				"meta.object-literal.key"
			],
			"settings": {
				"foreground": "#525252"
			}
		},
		{
			"scope": [
				"support.variable.property",
				"meta.type.parameters support.type"
			],
			"settings": {
				"foreground": "#E6E6E6",
				"fontStyle": "italic bold"
			}
		},
		{
			"name": "operators",
			"scope": [
				"source keyword.operator",
				"punctuation.separator",
				"punctuation.section"
			],
			"settings": {
				"foreground": "#4B7B81"
			}
		},
		{
			"name": "decorators, @property, __init__",
