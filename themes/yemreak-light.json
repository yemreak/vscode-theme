	"tokenColors": [
		{
			"scope": "source",
			"settings": {
				"foreground": "#3B3B3B"
			}
		},
		{
			"scope": "variable",
			"settings": {
				"foreground": "#3B3B3B",
				"fontStyle": "italic"
			}
		},
		{
			"name": "functions parameters",
			"scope": [
				"variable.parameter",
				"source variable.parameter",
				"meta.method.declaration",
				"source.pip-requirements",
				"meta.function-call.arguments",
				"meta.attribute",
				"meta.fstring"
			],
			"settings": {
				"foreground": "#484848",
				"fontStyle": "italic"
			}
		},
		{
			"scope": "markup.list.numbered.markdown",
			"settings": {
				"foreground": "#3B3B3B",
				"fontStyle": "italic"
			}
		},
		{
			"name": "functions",
			"scope": [
				"meta.definition.method",
				"meta.function.shell",
				"entity.name.function"
			],
			"settings": {
				"foreground": "#D7A10F"
			}
		},
		{
			"name": "functions calls",
			"scope": [
