				"keyword",
				"string.regexp"
			],
			"settings": {
				"foreground": "#4B7B81",
				"fontStyle": "italic"
			}
		},
		{
			"name": "none, null, undefined, imports, throw, exception",
			"scope": [
				"punctuation.accessor",
				"keyword.operator.logical",
				"meta.template.expression punctuation.definition",
				"keyword.operator.new",
				"entity.name.section",
				"entity.name.tag",
				"constant.language.python",
				"support.type.property-name.json"
			],
			"settings": {
				"foreground": "#E69A00"
